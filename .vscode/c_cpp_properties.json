{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/kernel/include",
                "${workspaceFolder}/kernel/include/acpi",
                "${workspaceFolder}/disktool/include",
                "${workspaceFolder}/libc/include"
            ],
            "compilerPath": "/usr/bin/x86_64-glidix-gcc",
            "cStandard": "gnu11",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-D__KERNEL__",
                "'-DKERNEL_VERSION=\"<KERNEL-VERSION>\"'",
                "-std=gnu11",
                "-Wall",
                "-D__glidix__",
                "-Werror",
                "-O3",
                "-nostdlib",
                "-D_GLIDIX_SOURCE"
            ]
        }
    ],
    "version": 4
}